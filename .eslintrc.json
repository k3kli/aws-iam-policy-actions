{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-unreachable":"error",
        "no-unused-vars":"error",
        "quotes": ["error", "single"]
    }
}
